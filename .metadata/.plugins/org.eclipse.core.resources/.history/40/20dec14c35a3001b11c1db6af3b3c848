package com.practice.bankingServices;

public class Transact {

	public static void main(String[] args) {
		BankingServices service1 = new BankingServices();
		BankingServices service2 = new BankingServices();
		BankingServices service3 = new BankingServices();
		BankingServices service4 = new BankingServices();
		BankingServices service5 = new BankingServices();
		BankingServices service6 = new BankingServices();
		
		Thread t1 = new Thread(service1);
		Thread t2 = new Thread(service2);
		Thread t3 = new Thread(service3);
		Thread t4 = new Thread(service4);
		Thread t5 = new Thread(service5);
		Thread t6 = new Thread(service6);
		
		t1.start();
		t2.start();
		t3.start();
		t4.start();
		t5.start();
		t6.start();
	}

}
