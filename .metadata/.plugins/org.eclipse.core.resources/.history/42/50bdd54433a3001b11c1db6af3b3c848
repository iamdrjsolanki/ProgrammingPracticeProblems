package com.practice.bankingServices;

import java.util.ArrayList;
import java.util.List;

public class BankingServices implements Runnable {
	
	private List<User> user;
	private List<Account> account;
	
	BankingServices() {
		user = new ArrayList<User>();
		for(int i=0; i<6; i++) {
			user.add(new User(i+1, "User"+(i+1)));
		}
		
		account = new ArrayList<Account>();
		for(int i=0; i<5; i++) {
			account.add(new Account("Account"+i));
		}
	}
	
	public void transfer(Account from, Account to, long amt) {}

	@Override
	public void run() {
		transfer
	}

}
